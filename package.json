{
	"name": "theme-tester",
	"displayName": "Theme Tester",
	"description": "Test a color theme without installing it",
	"version": "1.2.1",
	"publisher": "ms-vscode",
	"license": "MIT",
	"private": true,
	"engines": {
		"vscode": "^1.82.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-theme-tester"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-theme-tester/issues"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:vscode-theme-tester"
	],
	"contributes": {
		"commands": [
			{
				"command": "vscode-theme-tester.open",
				"title": "Theme Tester: Preview Theme"
			}
		],
		"resourceLabelFormatters": [
			{
				"scheme": "vscode-theme-tester",
				"formatting": {
					"label": "${path}",
					"separator": "/",
					"tildify": true,
					"workspaceSuffix": "Theme Tester Playground"
				}
			}
		]
	},
	"browser": "./dist/themeTesterMain.js",
	"scripts": {
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src/**/*.ts",
		"chrome": "npm run compile && vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --folder-uri=vscode-theme-tester://m/",
		"serve": "npm run compile && serve --cors -l 5000"
	},
	"devDependencies": {
		"@types/node": "18.x",
		"@types/vscode": "^1.82.0",
		"@types/webpack": "^5.28.5",
		"@typescript-eslint/eslint-plugin": "^8.44.1",
		"@typescript-eslint/parser": "^8.44.1",
		"@vscode/test-web": "^0.0.74",
		"assert": "^2.1.0",
		"eslint": "^9.36.0",
		"process": "^0.11.10",
		"request-light": "^0.8.0",
		"ts-loader": "^9.5.4",
		"typescript": "^5.9.3",
		"webpack": "^5.105.0",
		"webpack-cli": "^6.0.1",
		"serve": "^14.2.5"
	}
}